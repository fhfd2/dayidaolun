.background {
  width: 100%;
  height: 100%;
  position: fixed;
  background-size: 100% 100%;
  z-index: -1;
}

.section{
	margin-bottom: 10px;
}
.section_title{
	font-size: 16px;
	color:rgb(20, 22, 27);
	font-weight: bold;
}
.section_check{
	margin-right: 5px;
}
.section_tip{
	display: block;
	text-align: right;
	color: #5da2e2;
	padding-bottom: 10px;
}
.section_body{
	margin-top: 15px;
	align-items: center;
	padding-left: 10px;
	justify-content: center;
}